.model, .shijd {
	position: absolute;
	top: 0;
	bottom: 0;
	width: 100%;
	background-color: rgba(85, 85, 85, .5);
	z-index: 99999;
	display: none;
}

.model.active, .shijd.active{
	display: block;
}

.model .model-box {
	position: absolute;
	bottom: 0;
	width: 100%;
	text-align: center;
	transition: all .3s;
	transform: translateY(160px);
}

.model .model-box.active {
	transform: translateY(0);
}

.model .model-box li {
	height: 50px;
	line-height: 50px;
	padding: 0;
	border-bottom: 1px solid #ccc;
