.jfbtn-list .mui-col-xs-6 {
	height: 30px;
	line-height: 30px;
}

.jfbtn-list .mui-col-xs-6:nth-child(1):after {
	content: "";
	position: absolute;
	right: 0;
	width: 1px;
	height: 40px;
	background-color: #333;
}

.jfbtn-list .active:before {
	content: "";
	position: absolute;
	bottom: -5px;
	width: 100px;
	height: 1px;
	background-color: #666;
}

.jf-btn1.active button,
.jf-btn2.active button {
	background-color: #169BD5;
}

.jfbtn-list .mui-col-xs-6.active {
	font-weight: bolder;
}

.koufen,
