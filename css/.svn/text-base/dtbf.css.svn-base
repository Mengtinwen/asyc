.gl-slider .mui-segmented-control .mui-control-item {
	line-height: 20px;
}

.gl-slider a>p {
	margin: 0;
}

.gl-slider-span {
	width: 15px;
	height: 15px;
	border-radius: 50%;
	display: inline-block;
}

.gl-slider-span1 {
	background-color: #0072FF;
	/*蓝*/
}

.gl-slider-span2 {
	background-color: #FFEA00;
	/*黄*/
}

.gl-slider-span3 {
	background-color: #FF0B00;
	/*红*/
}

.gl-slider ul:before {
	height: 0;
}

.gl-slider ul li ul li {
	padding: 5px 10px;
}

.gl-slider ul li {
	border-bottom: 1px solid #ccc;
}

.gl-slider ul li ul li {
	border: 0;
}

.gl-kuang {
	width: 50px;
	height: 20px;
	border: 1px solid #666;
	display: inline-block;
	font-size: 12px;
	color: #666;
	text-align: center;
	line-height: 20px;
}

#gl-a .mui-active {
	color: #555;
}

.mui-navigate-right.font15.active:after {
	transform: rotateZ(90deg) translateX(-8px);
}

.mui-control-item.active1 p {
	color: #555;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.active1 {
	border-bottom: 2px solid #555;
}

.mui-control-item.active2 p {
	color: #0072FF;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.active2 {
	border-bottom: 2px solid #0072FF;
}

.mui-control-item.active3 p {
	color: #FFEA00;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.active3 {
	border-bottom: 2px solid #FFEA00;
}

.mui-control-item.active4 p {
	color: #FF0B00;
}

.mui-segmented-control.mui-segmented-control-inverted .mui-control-item.active4 {
