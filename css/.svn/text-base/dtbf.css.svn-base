.dt-foot {
	position: fixed;
	bottom: -165px;
	width: 100%;
	height: 214px;
	z-index: 999;
	background-color: #EDEDED;
	transition: all .3s;
}

.dt-foot .mui-row {
	height: 50px;
}

.dt-foot.active {
	bottom: 0;
}

.dtfoot-cont {
	width: 100%;
	height: 165px;
	padding: 6px 3px;
	background-color: #F2F2F2;
	overflow-y: auto;
}

.dt-foot .mui-pull-right a {
	margin-right: 20px;
}

.dtfoot-cont ul li {
	float: left;
}

.dtfoot-cont li {
	display: block;
	width: 40px;
	height: 40px;
	margin: 5px;
	border-radius: 50%;
	border: 1px solid #ccc;
	background-color: #fff;
	text-align: center;
	line-height: 38px;
}

.dt-foot button {
	width: 100%;
	height: 52px;
	padding: 0;
	border-radius: 0;
	background-color: #0000FF;
	color: #fff;
	font-size: 18px;
}

.jiaojuan, .shijd-box {
	position: absolute;
	left: 5%;
	top: 40%;
	width: 90%;
	height: 170px;
	border-radius: 10px;
	background-color: #fff;
	z-index: 9999;
	display: none;
}

.jiaojuan.active, .shijd-box.active {
	display: block;
}
.jiaojuan button,
.koufen button,
.bufen button {
	width: 100%;
	height: 40px;
	border-radius: 0;
}

.jj-button2 button {
	background-color: #DE2E44;
	color: #fff;
	border: 1px solid #DE2E44;
}
.shijd-box button{
	    width: 100%;
	    height: 40px;
	    padding: 0;
	    font-size: 16px;
	    color: #fff;
	    background-color: #F59A23;
}
.mui-backdrop {
	display: none;
}

.mui-backdrop.active {
	display: block;
}

.dtcj-top {
	height: 300px;
	background: -webkit-linear-gradient(left top, #FFFE7F, #F5A12A);
}

.transparent {
